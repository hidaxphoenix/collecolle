.split
  .split-item.split-left
    = render "posts/left-index"
  .split-item.split-right
    .split-right__inner
      .contents-box
        - @posts.each do |post|
          = render partial:"post",locals: { post: post }
        = paginate(@posts)
        -# %p{:style => "height:200px; width:100%;"}


  



















  


